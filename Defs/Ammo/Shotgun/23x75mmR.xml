			<armorPenetration>0.503</armorPenetration>
		</projectile>
	</ThingDef>

	<ThingDef ParentName="Base23x75mmRBullet">
		<defName>Bullet_23x75mmR_Beanbag</defName>
		<label>beanbag</label>
